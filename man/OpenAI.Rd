\if{html}{\out{<div class="r">}}\preformatted{OpenAI$find_file(..., as_df = TRUE)}\if{html}{\out{</div>}}
}

\subsection{Arguments}{
\if{html}{\out{<div class="arguments">}}
\describe{
\item{\code{...}}{Named arguments for filtering (e.g., \code{filename = "data.csv"},
\code{purpose = "assistants"}).}

\item{\code{as_df}}{Logical. Whether to return results as a data frame (default: TRUE).
If FALSE, returns a list.}
}
\if{html}{\out{</div>}}
}
\subsection{Returns}{
Data frame (or list) of matching files.
}
